{
	"plugins": [
		"angular"
	],
	"globals": {
		"angular": false,
		"_": false,
		"window": false,
		"Lazy":  false,
		// Angular Mocks
		"inject": false,
		// JASMINE
		"jasmine": false,
		"then": false,
		"moment": false,
		"describe": false,
		"fdescribe": false,
		"xdescribe": false,
		"it": false,
		"fit": false,
		"xit": false,
		"before": false,
		"beforeEach": false,
		"beforeAll": false,
		"after": false,
		"afterEach": false,
		"expect": false,
		"module": false,
		"mock": false,
		// Protractor
		"element": false,
		"by": false,
		"browser": false,
		"console": false,
		"spyOn": false,
		"require": false,
		"protractor": false
	},
	"ecmaFeatures": {
		"arrowFunctions": true,
		"binaryLiterals": true,
		"blockBindings": true,
		"classes": true,
		"defaultParams": true,
		"destructuring": true,
		"forOf": true,
		"generators": true,
		"globalReturn": true,
		"modules": true,
		"objectLiteralComputedProperties": true,
		"objectLiteralDuplicateProperties": true,
		"objectLiteralShorthandMethods": true,
		"objectLiteralShorthandProperties": true,
		"octalLiterals": true,
		"regexUFlag": true,
		"regexYFlag": true,
		"restParams": true,
		"spread": true,
		"superInFunctions": true,
		"templateStrings": true,
		"unicodeCodePointEscapes": true
	},
	"rules": {
		"no-extra-strict": 0,
		"no-console": 0,
		"quotes": [ 2, "single", "avoid-escape" ],
		"no-use-before-define": [ 2, "nofunc" ],
		"no-unused-vars": [
			2,
			{
				"vars": "all",
				"args": "none"
			}
		],
		"angular/ng_controller_name": [ 2, "/.*Ctrl$/" ],
		"angular/ng_no_digest": 0,
		"no-else-return": 2,
		"no-extra-parens": 2,
		"no-unexpected-multiline": 2,
		"default-case": 2,
		"dot-location": [ 2, "property" ],
		"no-throw-literal": 2,
		"radix": 2,
		"wrap-iife": [ 2, "outside" ],
		"brace-style": [
			2,
			"stroustrup",
			{ "allowSingleLine": true }
		],
		"comma-style": [ 2, "last" ]
	}
}
